<% if(alerts.error) { %>
  <% alerts.error.forEach(function(msg){ %>
    <div class="toast rounded">
      <%= msg %>
    </div>
    <br>
  <% }); %>
<% } %>
<% if(alerts.success) { %>
  <% alerts.success.forEach(function(msg){ %>
    <div class="toast rounded">
      <%= msg %>
    </div>
    <br>
  <% }); %>
<% } %>
