<div class="container">
<h2>Wishlist:</h2>
<div class="row">
<% results.forEach(function(recipe){%>
	<div class="col l3 m4 s12">
		<div class="row">
			<h5><%=recipe.title%></h5>
			<a href="/recipes/wishlist/<%=recipe.id%>" class="btn-floating btn-large waves-effect waves-light green accent-2"><i class="material-icons">info_outline</i></a>
			<a href="/recipes/wishlist/add/<%=recipe.id%>" class="btn-floating btn-large pink accent-2"><i class="material-icons">add</i></a>
			<a href="/recipes/wishlist/<%=recipe.id%>" class="btn-floating btn-large waves-effect waves-light red accent-2"><i class="material-icons">delete</i></a>
		</div>
		<div class="row">
			<img class="responsive-img" src="<%=recipe.img_url%>"></a>
		</div>
	</div>
<%})%>
</div>
</div>